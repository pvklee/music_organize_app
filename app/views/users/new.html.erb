<h1>Create your new account!</h1>

<%= render 'shared/errors' %>
<form action="<%= users_path %>" method="post">
    <%= auth_token %>
    
    <label for="user_email">Email</label>
    <input
        id="user_email"
        name="user[email]"
        type="text">
    <br>

    <label for="user_password">Password</label>
    <input
        id="user_password"
        name="user[password]"
        type="text">
    <br>

    <input type="submit" value="Make your account!"> 
</form>